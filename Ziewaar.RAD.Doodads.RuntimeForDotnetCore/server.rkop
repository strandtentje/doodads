<<>> : WebServer(["http://+:8243/"]) {
    : <f"site.rkop">;
    OnStarted->InformationMessage("Webserver Running");
    OnHead->SlimDump("[{requesttime}|{remoteip}] {method} {url}");
    OnStopping->InformationMessage("Webserver Stopping");
    OnException->!"message" : InformationMessage("Server complains {message}");
};